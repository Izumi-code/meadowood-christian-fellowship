<style>
    .slideshow {
        justify-content: center;
        align-items: center;
        position: relative;
        display: flex;
        width: 1500px;
        /* Adjust the width as needed */
        height: 700px;
        /* Adjust the height as needed */
    }
    
    .slideshow img {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        opacity: 0;
        transition: opacity 5.0s ease;
        animation: fade 35s infinite;
    }
    
    .slideshow img:nth-child(1) {
        animation-delay: 0s;
    }
    
    .slideshow img:nth-child(2) {
        animation-delay: 5s;
    }
    
    .slideshow img:nth-child(3) {
        animation-delay: 12s;
    }
    
    .slideshow img:nth-child(4) {
        animation-delay: 19s;
    }
    
    .slideshow img:nth-child(5) {
        animation-delay: 26s;
    }
    
    .slideshow img:nth-child(6) {
        animation-delay: 33s;
    }
    
    .slideshow img:nth-child(7) {
        animation-delay: 40s;
    }
    
    .slideshow img:nth-child(8) {
        animation-delay: 52s;
    }
    
    @keyframes fade {
        0% {
            opacity: 0;
        }
        20% {
            opacity: 1;
        }
        33.33% {
            opacity: 1;
        }
        53.33% {
            opacity: 0;
        }
        100% {
            opacity: 1;
        }
    }
    
    .gif {
        width: 100px;
        height: 100px;
        position: relative;
        animation-name: example;
        animation-duration: 15s;
        animation-iteration-count: infinite;
    }
    
    @keyframes example {
        0% {
            left: 0px;
            top: 100px;
        }
        25% {
            left: 1800px;
            top: 100px;
        }
        50% {
            transform: scaleX(-1);
            left: 1800px;
            top: 2800px;
        }
        75% {
            left: 0px;
            top: 2800px;
        }
        100% {
            left: 0px;
            top: 100px;
        }
    }
</style>

